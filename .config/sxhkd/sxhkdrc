#description: screenshot selesct
super + s
  screenshot select

#description: screenshot selesct
super + shift + s
  screenshot full

#description: powermenu
super + shift + x
  dpower

#description: file
super + e
  pcmanfm 

#description: browser
super + b
  firefox

#description: Emacs-CLient 
alt + e 
  emacs --daemon 

#description: rofi
super + d
  rofi -show drun 

#description: volume +
alt + l
  change-volume up 

#description: volume - 
alt + h
  change-volume down

#description: mute 
XF86AudioMute
  change-volume mute

#description: brightness + 
alt + k
  change-brightness up 

#description: brightness - 
alt + j
  change-brightness down

#description: Media keys 
alt + {Prior,Next}
  playerctl {previous,next}

alt + End
  playerctl play-pause

#description: restart sxhkd  
super + Escape
        pkill -USR1 -x sxhkd

#!/bin/bash

# invoke global X session script
if [ -d /etc/X11/xinit/xinitrc.d ]; then
  for f in /etc/X11/xinit/xinitrc.d/*; do
    [ -x "$f" ] && . "$f"
  done
  unset f
fi

xwallpaper --zoom $HOME/personal/photos\ \&\ videos/Wallpapers/nord-theme/GMV5L7.jpg &

#picom
picom --experimental-backends &

#Sxhkd
sxhkd &

#Dwmblocks
dwmblocks &

#dwm
exec dwm 
